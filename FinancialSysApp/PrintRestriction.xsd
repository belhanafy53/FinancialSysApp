                         Tbl_AccountingRestrictionItems.AccountingRestriction_ID, Tbl_AccountingRestrictionItems.AccountingRestrictionKind_ID AS Expr4, Tbl_Accounting_Guid.BrokerAccount, Tbl_Accounting_Guid.HighamountsAccount, 
                         Tbl_Accounting_Guid.PersonalAccount, Tbl_AccountingRestrictions_Kind.Name
